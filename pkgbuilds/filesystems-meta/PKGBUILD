# Maintainer: Dankcuddlybear <dankcuddlybear@proton.me>
pkgname=filesystems-meta
pkgver=$(date +%Y.%-m.%-d)
pkgrel=$(expr $(date +%-S) + $(expr 60 \* $(date +%-M)) + $(expr 3600 \* $(date +%-H)))
pkgdesc='Support for additional filesystems - all-in-one package'
arch=(any)
url='https://github.com/dankcuddlybear/pkgbuilds'
depends=(
	btrfs-progs
	cryptsetup
	dmraid
	dosfstools
	e2fsprogs
	exfatprogs
	f2fs-tools
	fscrypt
	lvm2
	mdadm
	mtools
	ntfs-3g
	squashfs-tools
	udftools
)
