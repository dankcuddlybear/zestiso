# Maintainer: Dankcuddlybear <dankcuddlybear@proton.me>
pkgname=archives-meta
pkgver=$(date +%Y.%-m.%-d)
pkgrel=$(expr $(date +%-S) + $(expr 60 \* $(date +%-M)) + $(expr 3600 \* $(date +%-H)))
pkgdesc='Support for additional compressed file types - all-in-one package'
arch=(any)
url='https://github.com/dankcuddlybear/pkgbuilds'
depends=(
	binutils
	bzip2
	bzip3
	cpio
	gzip
	libarchive
	lrzip
	lz4
	lzip
	lzop
	p7zip
	tar
	unrar
	unzip
	xz
	zip
	zstd
)
