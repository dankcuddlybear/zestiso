arch=(x86_64)
url='https://github.com/dankcuddlybear/zestiso'
conflicts=('nvidia-gpu-foss-meta' 'nvidia-gpu-pro-390xx-meta' 'nvidia-gpu-pro-470xx-meta' 'nvidia-gpu-pro-525xx-meta' 'nvidia-gpu-pro-meta')
depends=(libxnvctrl-340xx
	nvidia-340xx-dkms
	nvidia-340xx-settings
	nvidia-340xx-utils
	nvidia-prime
	ocl-icd
	opencl-nvidia-340xx
)
install=install.install
